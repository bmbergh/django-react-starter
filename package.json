{
  "name": "django-react-setup",
  "version": "1.0.0",
  "description": "Initial setup with django, react and webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --hot",
    "build": "webpack --config webpack.config.js --progress --colors",
    "build-production": "webpack --config webpack.prod.config.js --progress --colors",
    "build-dev": "webpack -p --define process.env.NODE_ENV='\"development\"' --progress --colors",
    "watch": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmbergh/django-react-starter.git"
  },
  "author": "Brandy Bergh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmbergh/django-react-starter/issues"
  },
  "homepage": "https://github.com/bmbergh/django-react-starter#readme",
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel": "6.5.2",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.21.0",
    "node-sass": "3.8.0",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-loader": "0.9.1",
    "react": "15.1.0",
    "react-hot-loader": "1.3.0",
    "sass-loader": "3.2.1",
    "webpack": "1.13.1",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.5.1",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0"
  }
}
